//factorial function
fn factorial(n: i32) -> i32 {
    if n == 0 {
        ret 1;
    } els {
        ret n * factorial(n - 1);
    }

    ret 0;
}

let fact := factorial(5);

let arr := [1,2,3,5];
arr[2];

print("Factorial of 5 is " + fact);

